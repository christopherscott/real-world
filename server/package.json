{
  "name": "real-world-server",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "playground": "graphql playground",
    "gen:types": "npx gql-gen --schema http://localhost:4000 --template graphql-codegen-typescript-template --out src/generated/types.ts src/schema.graphql",
    "jest": "jest",
    "test:server": "ts-node test/server.ts",
    "test": "npm-run-all --parallel start jest",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "async-retry": "^1.2.1",
    "download": "^7.1.0",
    "graphql-import": "^0.6.0",
    "graphql-tools": "^3.0.5",
    "graphql-yoga": "^1.14.12",
    "node-fetch": "^2.1.2",
    "prisma-binding": "^2.1.0",
    "query-string": "^6.1.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.2.1",
    "@types/download": "^6.2.2",
    "@types/jest": "^23.1.4",
    "@types/node-fetch": "^2.1.2",
    "@types/query-string": "^6.1.0",
    "@types/ramda": "^0.25.34",
    "dotenv-cli": "^1.4.0",
    "graphql-cli": "^2.16.4",
    "graphql-code-generator": "^0.10.1",
    "graphql-codegen-typescript-template": "^0.10.1",
    "graphql-request": "^1.6.0",
    "jest": "^23.3.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "prisma": "^1.11.0",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  }
}
