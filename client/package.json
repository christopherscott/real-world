{
  "name": "sk",
  "version": "1.0.0",
  "description": "real world",
  "main": "index.js",
  "scripts": {
    "start": "npx cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.dev.config.js",
    "build": "npx cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "gen:schema": "npx apollo-codegen introspect-schema http://localhost:4000 --output src/generated/schema.json",
    "gen:flow": "npx apollo-codegen generate 'src/**/*.js' --schema src/generated/schema.json --output src/generated/types.flow.js --target flow --addTypename --useFlowExactObjects --useFlowReadOnlyTypes",
    "types": "npx npm-run-all gen:*",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@reach/router": "^1.1.1",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.8",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-flow": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0-beta.4",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.75.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint-cli": "^4.7.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
